<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://code.alibabatech.com/schema/dubbo
        http://code.alibabatech.com/schema/dubbo/dubbo.xsd"
       >
    <dubbo:application name="dubboConsumer" />

    <dubbo:registry protocol="zookeeper" address="zookeeper://10.184.120.23:2181" />

    <dubbo:reference id="productService" interface="com.bnq.service.dubbo.IProductService" version="1.0.1" async="true" timeout="3000" group="dubbo"/>
    <dubbo:reference id="newProductService" interface="com.bnq.service.dubbo.IProductService" version="1.0.1" timeout="3000" group="provide"/>

    <dubbo:monitor protocol="registry"/>
</beans>