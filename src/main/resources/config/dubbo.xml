<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://code.alibabatech.com/schema/dubbo
        http://code.alibabatech.com/schema/dubbo/dubbo.xsd"
       >
    <dubbo:application name="provider"/>

    <dubbo:registry address="zookeeper://10.184.120.23:2181" />

    <dubbo:service interface="com.bnq.service.dubbo.IProductService" cluster="" ref="productService" group="dubbo" version="1.0.0" />
    <dubbo:service interface="com.bnq.service.dubbo.IProductService" ref="productService" group="dubbo" version="1.0.1" />
    <dubbo:service interface="com.bnq.service.dubbo.IProductService" ref="productService" group="provide" version="1.0.1" />

    <dubbo:protocol name="dubbo" host="127.0.0.1" port="20889" />

    <dubbo:protocol name="rest" port="8099" server="tomcat"/>

    <dubbo:monitor protocol="registry"/>

</beans>